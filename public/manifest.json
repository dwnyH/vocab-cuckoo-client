{
  "manifest_version": 2,

  "name": "vocab-cuckoo",
  "description": "vocabulary reminder chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "icons/birdIcon-16.png",
    "48": "icons/vocab-cuckoo48.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "storage",
    "alarms",
    "notifications",
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "oauth2": {
    "client_id": "1053057567351-dgtoo7j6vdii3up0s80hqimnh22qccvp.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU=' https://ajax.googleapis.com;; object-src 'self'",
  "key": ""
}
